<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#445069"
    tools:context=".articulo">


    <View
        android:id="@+id/view2"
        android:layout_width="259dp"
        android:layout_height="51dp"
        android:background="#252B48"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.795"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.944" />

    <View
        android:id="@+id/view"
        android:layout_width="380dp"
        android:layout_height="218dp"
        android:layout_marginTop="16dp"
        android:background="#252B48"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="188dp"
        android:layout_height="241dp"
        android:layout_marginTop="48dp"
        android:contentDescription="logo"
        android:src="@drawable/detodito"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="59dp"
        android:layout_height="103dp"
        android:layout_marginEnd="84dp"
        android:text="1"
        android:textColor="#FBFBFB"
        android:textSize="96sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.127" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="158dp"
        android:layout_height="44dp"
        android:layout_marginTop="32dp"
        android:text="Cantidad"
        android:textColor="#FBF9F9"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.877"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view" />

    <Button
        android:id="@+id/button2"
        android:layout_width="78dp"
        android:layout_height="88dp"
        android:layout_marginBottom="16dp"
        android:text="+"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.906"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button6"
        android:layout_width="82dp"
        android:layout_height="87dp"
        android:text="-"
        android:textSize="48sp"
        tools:layout_editor_absoluteX="141dp"
        tools:layout_editor_absoluteY="627dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="325dp"
        android:layout_height="62dp"
        android:layout_marginBottom="364dp"
        android:text="Snack Mix 50 g"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.29"
        app:layout_constraintStart_toStartOf="parent"
        tools:textColor="#FBFBFB" />

    <TextView
        android:id="@+id/descripcionText"
        android:layout_width="240dp"
        android:layout_height="76dp"
        android:layout_marginBottom="264dp"
        android:text="Precio $ 1.500"
        android:textColor="#FBF9F9"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.146"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="35dp"
        android:layout_height="45dp"
        android:text="1"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="255dp"
        tools:layout_editor_absoluteY="656dp"
        tools:textColor="#FDFCFC" />

</androidx.constraintlayout.widget.ConstraintLayout>